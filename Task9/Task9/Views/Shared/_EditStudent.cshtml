@model StudentUpdateDto
@{
    ViewBag.Title = "Change student profile data";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <style>
    </style>
</head>
<body>
    <h1>@ViewBag.Title</h1>
    <form action="/Student/EditStudent" method="post">
        <input type="hidden" name="id" value="@Model.Id" />
        <input type="hidden" name="groupId" value="@Model.GroupId" />
        <label for="firstName">FirstName:</label>
        <input type="text" name="firstName" id="firstName" value="@Model.FirstName" />
        <label for="lastName">LastName:</label>
        <input type="text" name="lastName" id="lastName" value="@Model.LastName" />
        <br />
        <input type="submit" value="Save" />
    </form>
</body>
</html>